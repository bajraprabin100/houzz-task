{
  "name": "houzz-task",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "prestart": "yarn format",
    "start": "set PORT=3006 &&  react-scripts start",
    "start:development": "env-cmd -f .env.development react-scripts start",
    "start:production": "env-cmd -f .env.production react-scripts start",
    "start:qa": "env-cmd -f .env.qa react-scripts start",
    "start:uat": "env-cmd -f .env.uat react-scripts start",
    "start:pte": "env-cmd -f .env.pte react-scripts start",
    "prebuild": "yarn format",
    "build": "react-scripts build",
    "build:development": "env-cmd -f .env yarn build",
    "build:production": "env-cmd -f .env.production yarn build",
    "build:training": "env-cmd -f .env.training yarn build",
    "build:qa": "env-cmd -f .env.qa yarn build",
    "build:uat": "env-cmd -f .env.uat yarn build",
    "build:pte": "env-cmd -f .env.pte yarn build",
    "postbuild": "copyfiles -u 1 deploy/* build",
    "test": "jest",
    "test:watch": "jest --watch",
    "e2e": "cypress",
    "e2e:open": "yarn e2e open",
    "e2e:run": "yarn e2e run",
    "eject": "react-scripts eject",
    "lint": "eslint . --ext .ts,.tsx --quiet",
    "lint:fix": "eslint . --ext .ts,.tsx --fix --quiet",
    "format": "prettier --write \"**/*.+(ts|tsx|json|md)\"",
    "gen": "plop",
    "gen:comp": "plop component",
    "gen:view": "plop view",
    "api:portal:gen": "restful-react import --file src/api/schemas/portal.schema.json --output src/api/portal.generated.tsx",
    "api:portal-config:gen": "restful-react import --file src/api/schemas/portal-config.schema.json --output src/api/portal-config.generated.tsx",
    "api:registration:gen": "restful-react import --file src/api/schemas/registration.schema.json --output src/api/registration.generated.tsx",
    "api:add-patient:gen": "restful-react import --file src/api/schemas/add-patient.schema.json --output src/api/add-patient.generated.tsx",
    "api:gen": "yarn api:portal:gen && yarn api:portal-config:gen && yarn api:registration:gen && yarn api:add-patient:gen",
    "postapi:gen": "yarn format",
    "gql:gen": "graphql-codegen --config graphql.config.yml",
    "postgql:gen": "yarn format"
  },
  "dependencies": {
    "@apollo/client": "^3.1.5",
    "@data-driven-forms/react-form-renderer": "^2.19.1",
    "@data-driven-forms/suir-component-mapper": "^2.22.0",
    "@microsoft/applicationinsights-react-js": "^3.4.0",
    "@microsoft/applicationinsights-web": "^2.8.7",
    "@okta/okta-auth-js": "^4.2.0",
    "@okta/okta-react": "^4.0.0",
    "@testing-library/jest-dom": "^5.11.5",
    "@testing-library/react": "^11.1.2",
    "@testing-library/user-event": "^12.2.2",
    "@types/google-map-react": "^2.1.7",
    "@types/googlemaps": "^3.43.3",
    "@types/react-test-renderer": "^16.9.3",
    "@types/redux-thunk": "^2.1.0",
    "@types/sanitize-html": "^1.27.1",
    "add": "^2.0.6",
    "ag-grid-community": "^24.1.0",
    "ag-grid-react": "^24.1.1",
    "apollo-link-rest": "^0.8.0-beta.0",
    "axios": "^0.21.0",
    "bootstrap": "^4.5.2",
    "connected-react-router": "^6.8.0",
    "env-cmd": "^10.1.0",
    "eslint": "^7.13.0",
    "final-form": "4.20.1",
    "google-map-react": "^2.2.0",
    "graphql": "^15.3.0",
    "graphql-anywhere": "^4.2.7",
    "highlight.js": "^11.1.0",
    "jest-junit": "^12.0.0",
    "lodash": "^4.17.20",
    "memoize-one": "^5.1.1",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.31",
    "normalizr": "^3.6.1",
    "prop-types": "^15.7.2",
    "qs": "^6.9.4",
    "react": "^17.0.1",
    "react-bootstrap": "^1.6.1",
    "react-bootstrap-icons": "^1.0.3-alpha5",
    "react-bootstrap-typeahead": "^5.1.1",
    "react-cookie": "^4.1.1",
    "react-dom": "^17.0.1",
    "react-dropzone": "^11.2.0",
    "react-final-form": "6.5.1",
    "react-google-autocomplete": "^2.7.2",
    "react-moment": "^1.0.0",
    "react-number-format": "^4.4.1",
    "react-paginate": "^6.4.0",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.0",
    "react-signature-pad-wrapper": "^1.3.0",
    "react-test-renderer": "^17.0.1",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0",
    "restful-react": "^15.1.2",
    "sanitize-html": "^2.3.2",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^2.0.3",
    "typescript": "~3.8.0",
    "yarn": "^1.22.4",
    "yup": "^0.29.3"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "~1.19.1",
    "@graphql-codegen/fragment-matcher": "~2.0.0",
    "@graphql-codegen/introspection": "~1.18.0",
    "@graphql-codegen/typescript": "~1.17.11",
    "@graphql-codegen/typescript-document-nodes": "^1.17.8",
    "@graphql-codegen/typescript-operations": "~1.17.9",
    "@graphql-codegen/typescript-react-apollo": "^2.0.6",
    "@types/faker": "^5.1.4",
    "@types/jest": "^26.0.15",
    "@types/lodash": "^4.14.161",
    "@types/moment": "^2.13.0",
    "@types/node": "^14.14.7",
    "@types/react": "^16.9.0",
    "@types/react-bootstrap-typeahead": "^3.4.6",
    "@types/react-dom": "^16.9.0",
    "@types/react-paginate": "^6.2.1",
    "@types/react-redux": "^7.1.9",
    "@types/react-router-dom": "^5.1.5",
    "@types/redux-mock-store": "^1.0.2",
    "@types/yup": "^0.29.4",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "copyfiles": "^2.3.0",
    "cypress": "^6.0.0",
    "cypress-dotenv": "^1.2.2",
    "dotenv": "^8.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.4",
    "faker": "^5.1.0",
    "husky": "^4.2.5",
    "ibm-openapi-validator": "^0.31.1",
    "jest-environment-jsdom": "^26.6.2",
    "lint-staged": "^10.2.11",
    "node-sass": "4.14.1",
    "plop": "^2.7.4",
    "prettier": "^2.0.5",
    "redux-mock-store": "^1.5.4",
    "ts-jest": "^26.4.4"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npx lint-staged"
    }
  },
  "jest": {
    "config": "jest.config.ts"
  },
  "lint-staged": {
    "*.+(ts|tsx)": [
      "eslint --fix",
      "git add"
    ],
    "*.+(json|css|md)": [
      "prettier --write",
      "git add"
    ]
  }
}
